  "description": "Simple tool to parse data on Google Play Market",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "^7.2"
  },
  "autoload": {
    "psr-0": {
